alias jpn='export LANG=ja_JP.UTF-8'
alias eng='export LANG=en_US.UTF-8'
alias ssd='sudo shutdown -h now'
alias ssr='sudo shutdown -r now'

alias g='git'
alias gs='git status'
alias gc='git commit'
alias gcm='git commit -m'
alias ga='git add'
alias gl='git log'
alias glp='git log -p'
alias glps='git log --pretty=short'

